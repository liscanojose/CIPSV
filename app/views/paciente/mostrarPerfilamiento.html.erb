        <td><%= @tipoFrustracion2 %></td>
        <td><%= @tipoFrustracion3 %></td>
        <td><%= @fechatipoFrustracion %></td>
      </tr>
    </table>
    
  </div>
  <!--     Fin      -->



<!--      Fustracion Por Impedimento       -->
<% if @tipoFrustracion1 != "No Aplica"%>
  <div class="row col-md-5 col-lg-10" style="text-align:center; margin:40px 0px -60px 0px; padding-left:100px;">
    <center><h3>Frustracion Por Impedimento</h3></center>
  </div>

 <div class="row col-sm-offset-3 col-md-5 col-lg-10" style="padding-left:100px;">
    <table class="show">
      <tr> <td class="table-title">Agresividad</td> <td><%=   @impedimento1  %></td></tr>
      <tr> <td class="table-title">Insensibilidad</td> <td><%= @impedimento2    %></td></tr>
