<%= render "partials/head_dashboard" %>

<div class="col-lg-11 col-md-3">
  <div class="col-lg-11">
    <h1 class="page-header">
      <%= @accionUsuario %> <small>Mostrar <%= @accionUsuario%></small>
    </h1>
  </div>

  <div class="row  col-lg-10" style="text-align:right;">
    <%= link_to 'Volver', url_for(:back) ,class:"btn btn-primary"%>
  </div>

 <div class="row col-sm-offset-2 col-md-5 col-lg-10" style="text-align:center;">
    <table class="show">
      <tr class="table-title">
        <td>Cedula</td>
        <td>Nombres</td>
        <td>Apellidos</td>
        <td>Tel√©fono</td>
        <!--<td></td>-->
      </tr>
      <tr>
        <td><%= @paciente.cedula %></td>
        <td><%= @paciente.nombres %></td>
        <td><%= @paciente.apellidos %></td>
        <td><%= @paciente.telefono %></td>
      </tr>
    </table>
  </div>

  <!--     Tipos de Fustraciones       -->
  <div class="row col-md-5 col-lg-10" style="text-align:center; margin:40px 0px -60px 0px; padding-left:100px;">
    <center><h3>Tipos de Frustraciones</h3></center>
  </div>

 <div class="row col-sm-offset-1 col-md-5 col-lg-10" style="padding-left:60px;">
