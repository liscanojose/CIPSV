<%= render "partials/head_dashboard" %>

   <div class="col-lg-11">
    <h1 class="page-header">
      <%= @accionUsuario %> <small>Mostrar <%= @accionUsuario%>s</small>
    </h1>
  </div>

  <div class="row col-sm-offset-2 col-md-5 col-lg-12" style="text-align:center;">
    <table class="show">
      <tr class="table-title">
    <td>Cedula</td>
    <td>Nombres</td>
    <td>Apellidos</td>
    <td></td>

  </tr>

  <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.cedula %></td>
        <td><%= paciente.nombres %></td>
        <td><%= paciente.apellidos %></td>
        
        <td><%= link_to 'Mostrar', paciente_path(paciente) %></td>
      </tr>
    <% end %>
    </table>
</div>
        
<%= render "partials/bottom_dashboard" %>